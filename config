# Domain list
# eg.
#   W21_ACME_DOMAIN_ARR=("My.domain1" "My.domain2")
export W21_ACME_DOMAIN_ARR=("example1.domain.com" "example2.domain.com")

# Your email
export W21_ACME_EMAIL="Your@mail.com"

# ISP Service
# See more: 
#   https://github.com/acmesh-official/acme.sh/wiki/dnsapi
export W21_ACME_DNS_ISP=dns_ali

# Use --dns dns_ali
export Ali_Key="Your Ali_Key"
export Ali_Secret="Your Ali_Secret"
